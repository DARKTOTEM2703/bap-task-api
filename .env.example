# Example environment variables (do NOT store real credentials here)
# Copy this file to `.env` locally and add real values in your machine only.

# MySQL Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=your_db_user
DB_PASS=your_secure_password_here
DB_NAME=your_db_name
TYPEORM_SYNC=false

# JWT Configuration
JWT_SECRET=your_min_32_chars_jwt_secret_key_here

# MinIO Configuration
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=your_minio_access_key
MINIO_SECRET_KEY=your_minio_secret_key
MINIO_BUCKET=your_bucket_name

# App
PORT=3000
NODE_ENV=development